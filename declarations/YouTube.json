{
  "name": "YouTube",
  "documents": {
    "Privacy Policy": {
      "fetch": "https://policies.google.com/privacy?hl=fr",
      "select": "div[role=\"article\"]",
      "remove": "img[src$='.svg'], img[src^='data:image/svg+xml']"
    },
    "Terms of Service": {
      "fetch": "https://www.youtube.com/t/terms",
      "select": "div#main-content"
    },
    "Community Guidelines": {
      "fetch": "https://www.youtube.com/intl/ALL_fr/howyoutubeworks/policies/community-guidelines/",
      "select": [".ytr-hero__details", ".ytr-article__content-wrapper"],
      "remove": [".ytr-hero__eyebrow", ".ytr-link > svg"],
      "sections": {
        "select": [".article-container"],
        "remove": [".scClientFeedbackInlineFeedbacklinkroot"],
        "Self-harm": {
          "fetch": "https://support.google.com/youtube/answer/2802245"
        },
        "Hate Speech": {
          "fetch": "https://support.google.com/youtube/answer/2801939"
        },
        "Child Exploitation": {
          "fetch": "https://support.google.com/youtube/answer/2801999"
        },
        "Violence Incitement": {
          "fetch": "https://support.google.com/youtube/answer/2802008"
        },
        "Violent Organizations": {
          "fetch": "https://support.google.com/youtube/answer/9229472"
        },
        "Spam": {
          "fetch": "https://support.google.com/youtube/answer/2801973"
        },
        "Regulated Goods": {
          "fetch": "https://support.google.com/youtube/answer/9229611"
        },
        "Harassment": {
          "fetch": "https://support.google.com/youtube/answer/2802268"
        },
        "Misinformation": {
          "fetch": "https://support.google.com/youtube/answer/10834785"
        },
        "Adult Nudity": {
          "fetch": "https://support.google.com/youtube/answer/2802002"
        },
        "Platform Manipulation": {
          "fetch": "https://support.google.com/youtube/answer/3399767"
        }
      }
    }
  }
}
